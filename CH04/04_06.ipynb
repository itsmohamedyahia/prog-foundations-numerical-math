    "from numpy import array\n",
    "from numpy.linalg import inv"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "5fbdef56",
   "metadata": {},
   "outputs": [],
   "source": [
    "#  2x-y+2z=6\n",
    "#  3x+2y+z=14\n",
    "#  x-y-z=0\n",
    "\n",
    "A = array([\n",
    "    [2, -1, 2],\n",
    "    [3, 2, 1],\n",
    "    [1, -1, -1]\n",
    "])\n",
    "B = array([6,14,0])\n",
    "\n",
    "X = inv(A).dot(B)"
   ]
  },
  {
